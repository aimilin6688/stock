<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.ruoyi.stock.mapper.StockMessageMapper">
    
    <resultMap type="StockMessage" id="StockMessageResult">
        <result property="id"    column="id"    />
        <result property="accountId"    column="account_id"    />
        <result property="body"    column="body"    />
        <result property="clientId"    column="client_id"    />
        <result property="type"    column="type"    />
        <result property="subject"    column="subject"    />
        <result property="createTime"    column="create_time"    />
        <result property="signTime"    column="sign_time"    />
        <result property="signClientId"    column="sign_client_id"    />
        <result property="weight"    column="weight"    />
        <result property="status"    column="status"    />
        <result property="executed"    column="executed"    />
        <result property="executedTime"    column="executed_time"    />
        <result property="executedResult"    column="executed_result"    />
        <result property="executedStatus"    column="executed_status"    />
        <result property="updateTime"    column="update_time"    />
        <result property="sendTime"    column="send_time"    />
        <result property="clientName"    column="client_name"    />
    </resultMap>

    <sql id="selectStockMessageVo">
        select id, account_id, client_id, type, subject, create_time, sign_time, sign_client_id, weight, status, executed, executed_time, update_time, send_time,executed_status,
               (select name from stock_client where stock_client.id = client_id) as client_name
               from stock_message
    </sql>

    <sql id="selectStockMessageDetailVo">
        select id, account_id,body, client_id, type, subject, create_time, sign_time, sign_client_id, weight, status, executed, executed_time,executed_result, update_time, send_time,executed_status,
               (select name from stock_client where stock_client.id = client_id) as client_name
        from stock_message
    </sql>

    <select id="selectStockMessageList" parameterType="StockMessage" resultMap="StockMessageResult">
        <include refid="selectStockMessageVo"/>
        <where>
            <if test="id != null "> and id = #{id}</if>
            <if test="accountId != null "> and account_id = #{accountId}</if>
            <if test="clientId != null "> and client_id = #{clientId}</if>
            <if test="type != null "> and type = #{type}</if>
            <if test="signClientId != null "> and sign_client_id = #{signClientId}</if>
            <if test="status != null "> and status = #{status}</if>
            <if test="executed != null "> and executed = #{executed}</if>
            <if test="sendTime != null "> and send_time = #{sendTime}</if>
        </where>
    </select>
    
    <select id="selectStockMessageById" parameterType="Long" resultMap="StockMessageResult">
        <include refid="selectStockMessageDetailVo"/>
        where id = #{id}
    </select>
        
    <insert id="insertStockMessage" parameterType="StockMessage" useGeneratedKeys="true" keyProperty="id">
        insert into stock_message
        <trim prefix="(" suffix=")" suffixOverrides=",">
            <if test="accountId != null">account_id,</if>
            <if test="body != null">body,</if>
            <if test="clientId != null">client_id,</if>
            <if test="type != null">type,</if>
            <if test="subject != null">subject,</if>
            <if test="createTime != null">create_time,</if>
            <if test="signTime != null">sign_time,</if>
            <if test="signClientId != null">sign_client_id,</if>
            <if test="weight != null">weight,</if>
            <if test="status != null">status,</if>
            <if test="executed != null">executed,</if>
            <if test="executedTime != null">executed_time,</if>
            <if test="executedResult != null">executed_result,</if>
            <if test="executedStatus != null">executed_status,</if>
            <if test="updateTime != null">update_time,</if>
            <if test="sendTime != null">send_time,</if>
         </trim>
        <trim prefix="values (" suffix=")" suffixOverrides=",">
            <if test="accountId != null">#{accountId},</if>
            <if test="body != null">#{body},</if>
            <if test="clientId != null">#{clientId},</if>
            <if test="type != null">#{type},</if>
            <if test="subject != null">#{subject},</if>
            <if test="createTime != null">#{createTime},</if>
            <if test="signTime != null">#{signTime},</if>
            <if test="signClientId != null">#{signClientId},</if>
            <if test="weight != null">#{weight},</if>
            <if test="status != null">#{status},</if>
            <if test="executed != null">#{executed},</if>
            <if test="executedTime != null">#{executedTime},</if>
            <if test="executedResult != null">#{executedResult},</if>
            <if test="executedStatus != null">#{executedStatus},</if>
            <if test="updateTime != null">#{updateTime},</if>
            <if test="sendTime != null">#{sendTime},</if>
         </trim>
    </insert>

    <update id="updateStockMessage" parameterType="StockMessage">
        update stock_message
        <trim prefix="SET" suffixOverrides=",">
            <if test="accountId != null">account_id = #{accountId},</if>
            <if test="body != null">body = #{body},</if>
            <if test="clientId != null">client_id = #{clientId},</if>
            <if test="type != null">type = #{type},</if>
            <if test="subject != null">subject = #{subject},</if>
            <if test="createTime != null">create_time = #{createTime},</if>
            <if test="signTime != null">sign_time = #{signTime},</if>
            <if test="signClientId != null">sign_client_id = #{signClientId},</if>
            <if test="weight != null">weight = #{weight},</if>
            <if test="status != null">status = #{status},</if>
            <if test="executed != null">executed = #{executed},</if>
            <if test="executedTime != null">executed_time = #{executedTime},</if>
            <if test="executedResult != null">executed_result = #{executedResult},</if>
            <if test="executedStatus != null">executed_status = #{executedStatus},</if>
            <if test="updateTime != null">update_time = #{updateTime},</if>
            <if test="sendTime != null">send_time = #{sendTime},</if>
        </trim>
        where id = #{id}
    </update>

    <delete id="deleteStockMessageById" parameterType="Long">
        delete from stock_message where id = #{id}
    </delete>

    <delete id="deleteStockMessageByIds" parameterType="String">
        delete from stock_message where id in 
        <foreach item="id" collection="array" open="(" separator="," close=")">
            #{id}
        </foreach>
    </delete>
    
</mapper>